                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_SIGNS_ATTACKS">
                <xs:annotation>
                    <xs:documentation>Sleep attacks (strong urge to sleep, followed by short nap)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" type="xs:boolean" default="false"/>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_SIGNS_CATA">
                <xs:annotation>
                    <xs:documentation>Cataplexy (sudden loss of muscle tone while awake, resulting in brief inability to
                        move)
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" type="xs:boolean" default="false"/>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_SIGNS_PARA">
                <xs:annotation>
                    <xs:documentation>Sleep paralysis (inability to move on first awakening)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" type="xs:boolean" default="false"/>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_SIGNS_DESCRIBE">
                <xs:complexType>
                    <xs:all>
