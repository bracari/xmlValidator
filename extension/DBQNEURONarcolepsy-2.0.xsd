                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_EPISODE_NUMBER">
                <xs:annotation>
                    <xs:documentation>Number of cataplectic (narcoleptic) episodes over past 6 months:
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" nillable="true">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="0-1"/>
                                    <xs:enumeration value="2 or more"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_EPISODE_NUMBER_MORE2">
                <xs:annotation>
                    <xs:documentation>If 2 or more over the past 6 months, indicate the average frequency of narcoleptic
                        episodes:
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" nillable="true">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="0-4 per week"/>
                                    <xs:enumeration value="5-8 per week"/>
                                    <xs:enumeration value="9-10 per week"/>
                                    <xs:enumeration value="More than 10 per week"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_EPISODE_DESCRIBE">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" type="xs:string" nillable="true"/>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_OPP_OTHER_PERT">
                <xs:annotation>
                    <xs:documentation></xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" nillable="true">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="Yes"/>
                                    <xs:enumeration value="No"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
