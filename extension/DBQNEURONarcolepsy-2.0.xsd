                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" nillable="true">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="0-4 per week"/>
                                    <xs:enumeration value="5-8 per week"/>
                                    <xs:enumeration value="9-10 per week"/>
                                    <xs:enumeration value="More than 10 per week "/>
                                </xs:restriction>
