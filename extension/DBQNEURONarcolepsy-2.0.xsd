                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_DT_IMAGING_OTHER_SIGN_DESCRIBE">
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" type="xs:string" nillable="true"/>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>

            <xs:element name="HISTORY_NEURO_NARC_FINDINGS_SIGNS_HALL_SLEEP">
                <xs:annotation>
                    <xs:documentation>Sleep onset/sleep offset hallucinations</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element name="Value" type="xs:boolean" default="false"/>
                        <xs:element name="Metadata" type="narco:Metadata"/>
                    </xs:all>
