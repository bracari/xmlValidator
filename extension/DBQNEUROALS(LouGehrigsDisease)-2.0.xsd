<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:als="http://vler.va.gov/vler/schemas/health/clinicalDocuments/clinicalAssessments/cpExams/AmyotrophicLateralSclerosis/2.0" targetNamespace="http://vler.va.gov/vler/schemas/health/clinicalDocuments/clinicalAssessments/cpExams/AmyotrophicLateralSclerosis/2.0" version="2.0" elementFormDefault="qualified">
	<xs:complexType name="AmyotrophicLateralSclerosisFormType">
		<xs:sequence>
			<xs:element name="EXAM_NEURO_ALS_NE_STR_WRST_EXT_LFT">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="5/5"/>
									<xs:enumeration value="4/5"/>
									<xs:enumeration value="3/5"/>
									<xs:enumeration value="2/5"/>
									<xs:enumeration value="1/5"/>
									<xs:enumeration value="0/5"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="AmyotrophicLateralSclerosis">
				<xs:annotation>
					<xs:documentation>Does the Veteran now have or has he/she ever been diagnosed with Amyotrophic Lateral Sclerosis (ALS)?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_DG_1">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_ICD_1">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_DATE_1">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_DG_2">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_ICD_2">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_DATE_2">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_DG_3">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_ICD_3">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_DATE_3">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="DIAGNOSIS_NEURO_ALS_ADD_DG">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_HX">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_HAND">
				<xs:annotation>
					<xs:documentation>b. Dominant hand</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Right"/>
									<xs:enumeration value="Left"/>
									<xs:enumeration value="Ambidextrous"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_MUSCLE">
				<xs:annotation>
					<xs:documentation>a. Does the Veteran have any muscle weakness in the upper and/or lower extremities attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_SWALLOW">
				<xs:annotation>
					<xs:documentation>b. Does the Veteran have any pharynx and/or larynx and/or swallowing conditions attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_SWALLOW_LIST">
				<xs:complexType>
					<xs:all>
						<xs:element name="Values">
							<xs:complexType>
								<xs:choice maxOccurs="8">            <xs:element name="Selected" nillable="true">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Constant inability to communicate by speech	  "/>
											<xs:enumeration value="Speech not intelligible or individual is aphonic"/>
											<xs:enumeration value="Paralysis of soft palate with swallowing difficulty (nasal regurgitation) and speech impairment"/>
											<xs:enumeration value="Hoarseness "/>
											<xs:enumeration value="Mild swallowing difficulties"/>
											<xs:enumeration value="Moderate swallowing difficulties"/>
											<xs:enumeration value="Severe swallowing difficulties, permitting passage of liquids only"/>
											<xs:enumeration value="Requires feeding tube due to swallowing difficulties"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_SWALLOW_LIST_OTH">
				<xs:annotation>
					<xs:documentation>Other, describe:</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_SWALLOW_LIST_OTH_DESC">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_RESP">
				<xs:annotation>
					<xs:documentation>c. Does the Veteran have any respiratory conditions attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_SLP_APNEA">
				<xs:annotation>
					<xs:documentation>d. Does the Veteran have signs and/or symptoms of sleep apnea or sleep apnea-like condition attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_SLP_APNEA_LST">
				<xs:complexType>
					<xs:all>
						<xs:element name="Values">
							<xs:complexType>
								<xs:choice maxOccurs="4">            <xs:element name="Selected" nillable="true">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Persistent daytime hypersomnolence "/>
											<xs:enumeration value="Requires use of breathing assistance device such as continuous positive airway pressure (CPAP) machine"/>
											<xs:enumeration value="Chronic respiratory failure with carbon dioxide retention or cor pulmonale"/>
											<xs:enumeration value="Requires tracheostomy"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_BOWEL">
				<xs:annotation>
					<xs:documentation>e. Does the Veteran have any bowel impairment attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_BOWEL_LST">
				<xs:complexType>
					<xs:all>
						<xs:element name="Values">
							<xs:complexType>
								<xs:choice maxOccurs="6">            <xs:element name="Selected" nillable="true">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Slight impairment of sphincter control, without leakage "/>
											<xs:enumeration value="Constant slight impairment of sphincter control, or occasional moderate leakage"/>
											<xs:enumeration value="Occasional involuntary bowel movements, necessitating wearing of a pad"/>
											<xs:enumeration value="Extensive leakage and fairly frequent involuntary bowel movements"/>
											<xs:enumeration value="Total loss of bowel sphincter control"/>
											<xs:enumeration value="Chronic constipation"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_BOWEL_LST_OTH">
				<xs:annotation>
					<xs:documentation>Other bowel impairment (describe):</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_BOWEL_LST_OTH_DESC">
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:string" nillable="true"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_LEAK">
				<xs:annotation>
					<xs:documentation>f. Does the Veteran have voiding dysfunction causing urine leakage attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_LEAK_LST">
				<xs:complexType>
					<xs:all>
						<xs:element name="Values">
							<xs:complexType>
								<xs:choice maxOccurs="4">            <xs:element name="Selected" nillable="true">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Does not require/does not use absorbent material"/>
											<xs:enumeration value="Requires absorbent material that is changed less than 2 times per day"/>
											<xs:enumeration value="Requires absorbent material that is changed 2 to 4 times per day"/>
											<xs:enumeration value="Requires absorbent material that is changed more than 4 times per day"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_FREQ">
				<xs:annotation>
					<xs:documentation>g. Does the Veteran have voiding dysfunction causing signs and/or symptoms of urinary frequency attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_FREQ_LST">
				<xs:complexType>
					<xs:all>
						<xs:element name="Values">
							<xs:complexType>
								<xs:choice maxOccurs="6">            <xs:element name="Selected" nillable="true">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Daytime voiding interval between 2 and 3 hours"/>
											<xs:enumeration value="Daytime voiding interval between 1 and 2 hours "/>
											<xs:enumeration value="Daytime voiding interval less than 1 hour"/>
											<xs:enumeration value="Nighttime awakening to void 2 times"/>
											<xs:enumeration value="Nighttime awakening to void 3 to 4 times "/>
											<xs:enumeration value="Nighttime awakening to void 5 or more times"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC">
				<xs:annotation>
					<xs:documentation>h. Does the Veteran have voiding dysfunction causing findings, signs and/or symptoms of obstructed voiding attributable to ALS?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_HES">
				<xs:annotation>
					<xs:documentation>Hesitancy</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_HES_CHKD">
				<xs:annotation>
					<xs:documentation>If checked, is hesitancy marked?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_SLOW">
				<xs:annotation>
					<xs:documentation>Slow or weak stream</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_SLOW_CHKD">
				<xs:annotation>
					<xs:documentation>If checked, is stream markedly slow or weak?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_DECR">
				<xs:annotation>
					<xs:documentation>Decreased force of stream</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_DECR_CHKD">
				<xs:annotation>
					<xs:documentation>If checked, is force of stream markedly decreased?</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" nillable="true">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_STRIC_1">
				<xs:annotation>
					<xs:documentation>Stricture disease requiring dilatation 1 to 2 times per year</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_STRIC_2">
				<xs:annotation>
					<xs:documentation>Stricture disease requiring periodic dilatation every 2 to 3 months</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
						<xs:element name="Metadata" type="als:Metadata"/>
					</xs:all>
				</xs:complexType>
			</xs:element>

			<xs:element name="HISTORY_NEURO_ALS_SAS_VOID_OBSTRUC_RECUR">
				<xs:annotation>
					<xs:documentation>Recurrent urinary tract infections secondary to obstruction</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Value" type="xs:boolean" default="false"/>
