    <!-- This is the document (root) type for the Cliam -->
    <xsd:complexType name="ClaimType">
        <xsd:annotation>
            <xsd:documentation>This is the document (root) type for the Claim</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="cld:DocumentTypeVersion"/>
                    <xsd:element ref="cld:ClaimID"/>
                    <xsd:element ref="cld:CommonData"/>
                    <xsd:element ref="cld:MedicalData"/>
                    <xsd:element ref="cld:Attachments"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="CommonDataType">
        <xsd:annotation>
            <xsd:documentation>Type to define the common claim level details.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="nc:Document"/>
                    <xsd:element ref="vler:Client"/>
                    <xsd:element ref="vler:ServiceProvider"/>
                    <xsd:element ref="nc:Person" minOccurs="2" maxOccurs="5000"/>
                    <xsd:element ref="nc:Facility"/>
                    <xsd:element ref="vler:ServiceProviderClientAssociation"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="CommonData" type="cld:CommonDataType">
        <xsd:annotation>
            <xsd:documentation>Common claim level details.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
