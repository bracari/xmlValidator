    <xsd:element name="MedicalData" type="cld:MedicalDataType">
        <xsd:annotation>
            <xsd:documentation>Exactly one selected claim.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="AttachmentsType">
        <xsd:annotation>
            <xsd:documentation>Type to define a binary attachments associated with the claim.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ComplexObjectType">
                <xsd:sequence>
                    <xsd:element ref="nc:Attachment" minOccurs="0" maxOccurs="5000"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="Attachments" type="cld:AttachmentsType">
        <xsd:annotation>
            <xsd:documentation>Zero or more attachments associated with the selected claim.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Claim" type="cld:ClaimType">
        <xsd:annotation>
            <xsd:documentation>This exchange schema describes the Claim captured
                and submitted from the DBQ System to be exchanged with VLER
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="DocumentTypeVersion" type="nc:TextType">
        <xsd:annotation>
            <xsd:documentation>The version number of the current DBQ Schema</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
