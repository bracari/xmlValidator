	<!-- This is the document (root) type for the Cliam -->
	<xsd:complexType name="ClaimType">
		<xsd:annotation>
			<xsd:documentation>This is the document (root) type for the Claim</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="cld:DocumentTypeVersion" minOccurs="1"/>
					<xsd:element ref="cld:ClaimID" minOccurs="1"/>
					<xsd:element ref="cld:CommonData" minOccurs="1"/>
					<xsd:element ref="cld:MedicalData" minOccurs="1"/>
					<xsd:element ref="cld:Attachments" minOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CommonDataType">
		<xsd:annotation>
			<xsd:documentation>Type to define the common claim level details.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:Document" minOccurs="1"/>
					<xsd:element ref="vler:ExamDetail" minOccurs="1"/>
					<xsd:element ref="vler:Client" minOccurs="1"/>
					<xsd:element ref="vler:ServiceProvider" minOccurs="1"/>
					<xsd:element ref="vler:Approver" minOccurs="0"/>
					<xsd:element ref="nc:Person" minOccurs="2" maxOccurs="5000"/>
					<xsd:element ref="nc:Facility" minOccurs="1"/>
					<xsd:element ref="vler:ServiceProviderClientAssociation" minOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="CommonData" type="cld:CommonDataType">
		<xsd:annotation>
			<xsd:documentation>Common claim level details.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
